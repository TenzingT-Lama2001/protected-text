{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Default",
    "compilerOptions": {
      "target": "ES2022",                                     
      "module": "Node16",                                
      "moduleResolution": "node16",                             
      "removeComments": true,                              
      "esModuleInterop": true,   
      "resolveJsonModule": true,                         
      "forceConsistentCasingInFileNames": true,            
      "strict": true,                                      
      "skipLibCheck": true
    }
}
