{
  "name": "my-turborepo",
  "packageManager": "yarn@1.22.19",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "private": true,
  "scripts": {
    "build": "turbo run build",
    "dev": "turbo run dev",
    "lint": "turbo run lint",
    "test":"turbo run test",
    "format": "prettier --write \"**/*.{ts,tsx,md}\"",
    "prepare": "husky install"
  },
  "devDependencies": {
    "eslint": "^8.48.0",
    "eslint-config-custom": "*",
    "husky": "^8.0.0",
    "prettier": "^3.0.3",
    "tsconfig": "*",
    "turbo": "latest",
    "jest": "^29.6.2"
  }
}
